  $(window).scroll(function(){
    // get the height of #wrap
    var a = $("#body").height();
    var b = $(window).scrollTop();
    if(a > 2950)
    {
      if( b > 2950 && b <= (a) )
      {
          if($.cookie('no_body_related') != "true" && $('#notice').length != 0)
          {
            $("#notice").fadeIn("slow");
            //_gaq.push(['_trackEvent', 'Also Read', 'Show', 'Displayed Box', 0, true]);
          }
      }
      else
      {
        $("#notice").fadeOut("slow");
      }
    }
  });
  $('.close_button').click( function(){$("#notice").fadeOut("slow"); $.cookie("no_body_related", "true", { path: '/', expires: 1});});

 //New Stuff Section additions...
  if ($('.editorial-bottom').length)
  {
    $('#zergnet').css("margin-left","-15px");
    $('#mainContent .content').first().append($('#zergnet'));
    $('#repost_us_button_div').fadeIn("slow");
    $('#zergnet').fadeIn("slow");
    if ($('.disqus_box').length)
    {
      $('.disqus_box').after($('#new_stuff_bottom_section_content'));
      $('.disqus_box').after($('.stacked_bottom_unit'));
      $('#new_stuff_bottom_section_content').fadeIn("slow");
      $('.stacked_bottom_unit').fadeIn("slow");
    }
    else
    {
      $('.editorial-bottom').after($('#new_stuff_bottom_section_content'));
      $('.editorial-bottom').after($('.stacked_bottom_unit'));
      $('#new_stuff_bottom_section_content').fadeIn("slow");
      $('.stacked_bottom_unit').fadeIn("slow");

    }
  }
  else
  {
    if ($('.bottom_small').length)
    {
      if ($('.more_cheats').length)
      {
        $('.more_cheats').after($('#new_stuff_bottom_section'));
        $('.more_cheats').after().append($('#zergnet'));
        $('#repost_us_button_div').fadeIn("slow");
        $('#zergnet').fadeIn("slow");
        $('#new_stuff_bottom_section').fadeIn("slow");
      }
      else
      {
        $('#mainContent .content').first().append($('#zergnet'));
        $('#repost_us_button_div').fadeIn("slow");
        $('#zergnet').fadeIn("slow");
        if ($('.disqus_box').length)
        {
          $('.disqus_box').after($('#new_stuff_bottom_section_content'));
          $('#new_stuff_bottom_section_content').fadeIn("slow");
        }
        else
        {
          if ($('#mainContent .bottom').length)
          {
            $('#mainContent .bottom').last().after($('#new_stuff_bottom_section_content'));
            $('#new_stuff_bottom_section_content').fadeIn("slow");
          }
          else
          {
            $('.bottom_small').after($('#new_stuff_bottom_section_content'));
            if(!$('.pollResultsTitleRow').length)
            {
              $('#new_stuff_bottom_section_content').fadeIn("slow");
            }
          }
        }
      }
    }
  }

  level=0;
  if($.cookie('content-ad-level') == "0" || $.cookie('content-ad-level') == null)
  {
      level=0;
      $.cookie("content-ad-level", "1", { path: '/' });
  }
  else if ($.cookie('content-ad-level') == "1")
  {
      level=4;
      $.cookie("content-ad-level", "2", { path: '/' });
  }
  else if($.cookie('content-ad-level') == "2")
  {
      level=8;
      $.cookie("content-ad-level", "3", { path: '/' });
  }
  else if($.cookie('content-ad-level') == "3")
  {
      level=12;
      $.cookie("content-ad-level", "4", { path: '/' });
  }
  else
  {
      level=0;
      $.cookie("content-ad-level", "1", { path: '/' });
  }

  $('.content-bottom-ad-specials').each(function(index) {
    $(this).show();
  });

  $('.content-ad-specials').each(function(index) {
      if(index == 0+level)
      {
          $(this).show();
          $(".more_content_ad").show();
      }
      if(index == 1+level)
      {
          $(this).show();
          $(".more_content_ad").show();
      }
      if(index == 2+level)
      {
          $(this).show();
          $(".more_content_ad").show();
      }
      if(index == 3+level)
      {
          $(this).show();
          $(".more_content_ad").show();
      }
  });
});
$('head').append("<meta content='2a2cbb258183d9ea1d396f292df44c7d' name='RepostUsAPIKey' />");
</script>



