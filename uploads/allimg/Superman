    $(window).scroll(function(){
        var SCTop=$(window).scrollTop();
        if(SCTop>470){
            $(".fixed-header").show();
            $(".fixed-header").css({"position":"fixed", "top":"0", "left":"0"});
        }
        else{
            $(".fixed-header").hide();
            $(".fixed-header").css({"position":"inherit", "top":"auto", "left":"auto"});
        }
    });

