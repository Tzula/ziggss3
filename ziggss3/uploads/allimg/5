<script type="text/javascript">
    var nid = 5576;
    var rtid = 0;
    function news_like(p) {
        $.ajax({
            type: "POST",
            url: "/ajax/like",
            data: "id=" + nid + "&p=" + p,
            success: function(msg){
                if(msg == "OK") {
                    notice("Thanks for your vote.");
                } else {
                    notice(msg);
                }
            }
        });
    }
    function select_reply(rid, area, name, r) {
        var t = $("#reply_" + area + "_" + rid);
      //  t.parents('.small_commnet').css('display','block');
        t.parents('.small_commnet').toggle();
        t.focus();
        if(!r) {
            rtid = 0;
        } else {
            rtid = r;
        }
        if(name) {
            t.val("Reply to " + name + " :");
        }
    }
    function post_reply(rid, area) {
        var t = $("#reply_" + area + "_" + rid);
        var v = t.val();
        var login = '';
        if(!v || v=="Add a comment..." || v=="Enter text...") {
            notice("What's your opinion?");
            t.focus();
        }else if(!login){
            notice("Not Login");
        }else {
            t.val("");
            $.ajax({
                type: "POST",
                url: "/ajax/comment",
                data: "news_id="+nid+"&reply_id="+rid+"&rid="+rtid+"&content="+v+"&a="+area,
                success: function(msg){
                    if(msg == "OK") {
                        var html = '';
                        var src = 'http://s.orzzzz.com/avatars/0/.jpg';
                        var username = 'Guest';
                        var time = '03/03/16 05:27am';
                        html += '<li>'+
                                '<a> <img src="'+src+'" class="avatar_user"> </a>'+
                                '<div class="commnet_info">'+
                                    '<div class="name_time clearfix">'+
                                         '<a> <span class="name">'+username+'</span></a>'+
                                         '<span class="time">'+time+'</span>'+
                                     '</div>'+
                                     '<div class="commnet_info_main">'+v+'</div>'+
                                     '<div class="commnet_info_footer clearfix">';
                            if(area == 'head'){
                                    html+='      <span  class="commentbox">Comment</span>'+
                                               '<span class="dislikes">Dislike(0)</span>'+
                                               ' <span  class="likes">Like(0)</span>';
                            }
                            html += '</div>'+
                               '</div>'+
                               '</li>';

                        if(area == 'head'){
                            $(".discus_comment").prepend(html);
                            var num = parseInt($(".view-href b").html());
                            num+=1 ;
                            $(".view-href b").html(num);
                            $(".color_8a8a").html('('+num+')');
                        }else{
                            $("#reply_commnet_list_"+rid).append(html);
                        }

                        notice("Thanks for your comment.");
                       // t.val("");
                    } else {
                        notice(msg);
                    }
                    rtid = 0;
                }
            });
        }
    }
    function comment_like(rid, p) {
        $.ajax({
            type: "POST",
            url: "/ajax/commentLike",
            data: "id=" + rid + "&p=" + p,
            success: function(msg){
                if(msg == "OK") {
                    notice("Thanks for your vote.");
                } else {
                    notice(msg);
                }
            }
        });
    }
    function h2l(obj) {
        s = obj.html().toLowerCase();
        if(!s) s = "";
        return s;
    }
    function hl_pg(obj, p) {
        var o = $(obj);
        var op = $(obj).parent();
        var c = op.find("a[class~='select']");
        h = h2l(o);
        op.find("a").removeClass('select');
        if(h.indexOf("page") == -1) {
            o.addClass('select');
        } else {
            if(h.indexOf("next") != -1) {
                if(h2l(c.next()).indexOf('page') != -1) {
                    c.addClass('select');
                    return 0;
                }
                c.next().addClass('select');
                p = parseInt(c.html()) + 1;
            } else {
                if(h2l(c.prev()).indexOf('page') != -1) {
                    c.addClass('select');
                    return 0;
                }
                c.prev().addClass('select');
                p = parseInt(c.html()) - 1;
            }
        }
        return p;
    }
    function comment_reply_pg(rid, p, a, obj) {
        p = hl_pg(obj, p);
        if(!p) {
            return false;
        }
        $.ajax({
            type: "POST",
            url: "/ajax/replyPage",
            data: "id="+rid+"&p="+p+"&a="+a,
            success: function(msg){
                $("#reply_commnet_" + a + "_" + rid).html(msg);
            }
        });
    }
    function comment_pg(nid, p, obj) {
        p = hl_pg(obj, p);
        if(!p) {
            return false;
        }
        $.ajax({
            type: "POST",
            url: "/ajax/commentPage",
            data: "id=" + nid + "&p=" + p,
            success: function(msg){
                $("#comment_list").html(msg);
            }
        });
    }
    function notice(msg) {
        alert(msg);
    }
</script>
