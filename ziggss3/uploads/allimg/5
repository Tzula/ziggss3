<script>
    $(function(){
        var newspage = 2;
        var sentIt = true;
        lTop=$('#loading-more').offset().top;
        wH=$(window).height();
        $(window).scroll(function() {
            var sTop=$(window).scrollTop();
            var s=lTop-sTop-wH;
            if(s<-150 && sentIt){
                $.ajax({
                    type: "POST",
                    url: "/ajax/newslist",
                    data: "page=" + newspage ,
                    beforeSend: function(){sentIt = false; }, // sentIt false
                    success: function(msg){
                        $(".recommended-list-news ul").append(msg);
                        newspage+=1;
                    },
                    complete: function(){setTimeout(function(){sentIt = true;}, 600); } // sentIt true
                });
            }
        });
    })
</script>
